{
  "name": "gentelella-admin-dashboard",
  "version": "1.0.0",
  "description": "A modern Admin Dashboard template built with Gentelella, Bootstrap 5, Vite, Chart.js, and ECharts",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:watch": "vite --watch",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,html}",
    "analyze": "vite build --mode analyze"
  },
  "keywords": [
    "admin-dashboard",
    "bootstrap",
    "vite",
    "chart.js",
    "echarts",
    "gentelella"
  ],
  "author": "Gentelella Admin Dashboard Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/gentelella-admin-dashboard.git"
  },
  "homepage": "https://your-username.github.io/gentelella-admin-dashboard",
  "bugs": {
    "url": "https://github.com/your-username/gentelella-admin-dashboard/issues"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^6.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.1",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.0",
    "datatables.net": "^2.2.2",
    "datatables.net-bs5": "^2.2.2",
    "echarts": "^6.0.0",
    "jquery": "^3.7.1",
    "weather-icons": "^1.3.2"
  }
}
